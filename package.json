{
  "name": "single-market-robot-simulator-db-googledrive",
  "version": "2.4.0",
  "description": "settings and data db for single-market-robot-simulator studies with storage on Google Drive",
  "main": "build/index.js",
  "jsnext:main": "src/index.js",
  "scripts": {
    "clean": "rm -rf ./build ./doc ; mkdir ./build",
    "build": "node_modules/.bin/babel src --out-dir build",
    "doc": "node_modules/.bin/esdoc -c esdoc.json",
    "lint": "node_modules/.bin/eslint src/**.js",
    "go": "npm run clean && npm run lint && npm run build"
  },
  "author": "drpaulbrewer@eaftc.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/drpaulbrewer/single-market-robot-simulator-db-googledrive"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.0",
    "@babel/polyfill": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/register": "^7.10.5",
    "coveralls": "^3.1.0",
    "esdoc": "^1.1.0",
    "eslint": "^7.6.0"
  },
  "dependencies": {
    "array-prefer": "^0.1.0",
    "p-reduce": "^2.1.0",
    "search-string-for-google-drive": "^1.0.0",
    "secure-json-parse": "^2.1.0",
    "single-market-robot-simulator-db-studyfolder": "^2.0.0",
    "single-market-robot-simulator-db-zip": "^2.1.0"
  }
}
